create_table("scmproviders") {
	t.Column("id", "uuid", {primary: true})
	t.Column("name", "string")
	t.Column("user_id","uuid")
	t.Column("scmprovider_token", "string")
	t.Column("scmprovider_id", "string")
}

add_foreign_key("scmproviders", "user_id", {"users": ["id"]}, {
    "name": "scmproviders_user_fk",
    "on_delete": "CASCADE",
    "on_update": "CASCADE",
})